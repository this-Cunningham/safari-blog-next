{
  "extends": "next/core-web-vitals",
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": [
          "**/tests/**",
          "**/__tests__/**"
        ]
      }
    ],
    "no-console": "off",
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_"
      }
    ]
  }
}
